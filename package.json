  },
  "license": "MIT",
  "author": "SchemeSonic",
  "scripts": {
    "build": "NODE_ENV=production rimraf dist/* && webpack --config=configs/webpack/prod.js",
    "start": "source $HOME/.nvm/nvm.sh; nvm use --delete-prefix; npm run start-dev",
    "start-dev": "NODE_ENV=development webpack serve --config=configs/webpack/dev.js --open",
    "start-prod": "NODE_ENV=production npm run build && node express.js",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "prepare": "husky install"
  },
  "dependencies": {
