{
  "name": "paraboly-react-boilerplate",
  "version": "1.0.0",
  "description": "Paraboly React Boilerplate",
  "keywords": [
    "paraboly",
    "react",
    "boilerplate"
  ],
  "homepage": "https://github.com/parabol/paraboly-react-boilerplate",
  "bugs": {
    "url": "https://github.com/parabol/paraboly-react-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parabol/paraboly-react-boilerplate.git"
  },
  "license": "MIT",
  "author": "SchemeSonic",
  "scripts": {
    "build": "npm run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "source $HOME/.nvm/nvm.sh; nvm use --delete-prefix; npm run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js --open",
    "start-prod": "npm run build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
