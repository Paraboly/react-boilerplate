{
  "name": "@paraboyl/react-boilerplate",
  "version": "1.0.0",
  "description": "Paraboly React Boilerplate",
  "keywords": [
    "paraboly",
    "react",
    "boilerplate"
  ],
  "homepage": "https://github.com/Paraboly/react-boilerplate",
  "bugs": {
